<experiment>
    <id>diss60000001</id>
    <variables>
        <nameprefix>5</nameprefix>
        <distance>cityblk</distance>
        <printing>2</printing>
		<select_type>3</select_type>
        <feature>
            <name>LBP</name>
            <radius>1</radius>
            <samples>8</samples>
            <type>u2</type>
			<bins>12</bins>
			<winSize>7</winSize>
			<decorr>1</decorr>
			<freqestim>1</freqestim>
			<mode>h</mode>
			<gabor_o>8</gabor_o>
			<gabor_s>5</gabor_s>
			<vectors>500</vectors>
        </feature>
        <patches>
            <type>nonover</type>
            <size_x>20</size_x>
            <size_y>20</size_y>
        </patches>
        <data>
            <channel>gray</channel>
            <max_x>200</max_x>
            <max_y>200</max_y>
            <histeq>1</histeq>
			<mean_window>1</mean_window>
            <ellipse_x>0</ellipse_x>
            <ellipse_y>0</ellipse_y>
            <ellipse_a>0</ellipse_a>
            <ellipse_b>0</ellipse_b>
            <ellipse_t>0</ellipse_t>
			<ellipse_type>0</ellipse_type>
        </data>
    </variables>
    <input>
		<training></training>
		<training_id></training_id>
        <probe>/imagelists/2012_03_Dissertation/frgc_lefteye_exp1.srt</probe>
		<probe_id>/imagelists/2012_03_Dissertation/frgc_lefteye_exp1_ids.srt</probe_id>
        <gallery>/imagelists/2012_03_Dissertation/frgc_lefteye_exp1.srt</gallery>        
		<gallery_id>/imagelists/2012_03_Dissertation/frgc_lefteye_exp1_ids.srt</gallery_id>
        <datadir>/Processed_Images/FRGC/FRGC_PERI/</datadir>
    </input>
    <output>
        <resultsdir>/results/Dissertation_0_2/</resultsdir>
		<training_mat></training_mat>
        <probe_mat></probe_mat>
        <gallery_mat>gallery.mat</gallery_mat>
		<distance_mat>distance.mat</distance_mat>
		<results_mat>results.mat</results_mat>
		<roc_resolution>5000</roc_resolution>
		<make_files>1</make_files>
		<make_figures>0</make_figures>
		<plot_color>r</plot_color>
		<plot_linewidth>2</plot_linewidth>
    </output>
</experiment>
